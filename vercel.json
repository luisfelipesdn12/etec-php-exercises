{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.4.0"
        },
        "api/**/*.php": {
            "runtime": "vercel-php@0.4.0"
        }
    },
    "build": {
        "api/public/**": {
            "runtime": "@vercel/static"
        },
        "api/**/public/**": {
            "runtime": "@vercel/static"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.php"
        }
    ]
}